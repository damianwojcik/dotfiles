{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-var": 0,
        "linebreak-style": 0,
        "object-shorthand": 0,
        "func-names": 0,
        "camelcase": 1
    },
    "env": {
        "browser": true,
        "node": true
    }
}
